<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controlleres.BudgetController">
   <children>
      <HBox layoutX="10.0" layoutY="10.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="500.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="40.0" prefWidth="500.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="37.0" prefWidth="136.0" styleClass="labelText" stylesheets="@../../Css/pagStyle.css" text="العام المالي :" />
                        <TextField fx:id="fiscalyearname" alignment="CENTER" nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0" prefWidth="310.0" styleClass="textStyle" stylesheets="@../../Css/pagStyle.css" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <HBox prefHeight="38.0" prefWidth="500.0">
                     <children>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#creatAccount" prefHeight="30.0" prefWidth="150.0" styleClass="pageButtonStyle" stylesheets="@../../Css/pagStyle.css" text="انشاء حساب للسنة المالية" HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets left="2.0" />
                           </HBox.margin>
                        </Button>
                        <Button layoutX="12.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#editData" prefHeight="30.0" prefWidth="150.0" styleClass="pageButtonStyle" stylesheets="@../../Css/pagStyle.css" text="تعديل " HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets left="2.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#closeYearBudgetAccount" prefHeight="50.0" prefWidth="200.0" styleClass="pageButtonStyle" stylesheets="@../../Css/pagStyle.css" text="اغلاق ميزانية السنة الحالية وترحيل الرصيد" />
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </VBox>
            <TableView fx:id="balance" nodeOrientation="RIGHT_TO_LEFT" prefHeight="200.0" prefWidth="200.0" styleClass="tableStyle" stylesheets="@../../Css/pagStyle.css" HBox.hgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="fiscalyearname_col" prefWidth="75.0" text="العام المالي" />
                  <TableColumn fx:id="year_col" prefWidth="75.0" text="العام" />
                  <TableColumn fx:id="currentbalance_col" prefWidth="75.0" text="رصيد العام المالي" />
                  <TableColumn fx:id="carryoverbalance_col" prefWidth="75.0" text="الرصيد المرحل" />
                  <TableColumn fx:id="totalbalane_col" prefWidth="75.0" text="الرصيد الكلي" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>
